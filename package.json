{
  "name": "phone-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "cross-env react-scripts start",
    "build": "cross-env react-scripts build",
    "test": "cross-env react-scripts test",
    "eject": "cross-env react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
