  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^5.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3"
  }
}
